protocol bgp bio_public_self_core2 from bio_public {
  neighbor 44.16.51.242 as 202540;
  source address 44.16.51.250;
  import filter {
    if public_is_self_net() then {
      accept;
    }
    reject;
  };
  export filter {
    if is_rfc1918() then {
      reject;
    }
    if bgp_path.len <= 2 then {
      accept;
    }
    if public_is_self_net() then {
      accept;
    }
    reject;
  };
  next hop self;
};
