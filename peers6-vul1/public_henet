protocol bgp be_public_henet from be_public {
  neighbor 2001:470:12:4d::1 as 6939;
  import filter {
    if is_default() then reject;
    if public_is_self_net() then reject;
    if public_is_choopa() then reject;
    accept;
  };
  export filter {
    if public_is_self_net_agg() then {
      accept;
    }
    reject;
  };
};
